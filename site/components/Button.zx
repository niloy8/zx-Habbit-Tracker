pub const ButtonProps = struct {
    text: []const u8,
    href: []const u8,
    class: []const u8,
};

pub fn Button(allocator: zx.Allocator, props: ButtonProps) zx.Component {
    return (
        <a @allocator={allocator} href={props.href} class={props.class}>
            {props.text}
        </a>
    );
}

const zx = @import("zx");
